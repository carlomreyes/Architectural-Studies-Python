   "source": [
    "# Simple L-system replacement example\n",
    "rules = {'A':'AB','B':'A'}\n",
    "seq = 'A'\n",
    "for i in range(4):\n",
    "    seq = ''.join(rules.get(ch,ch) for ch in seq)\n",
    "print(seq)"
